# API URL - Production API endpoint
REACT_APP_API_URL=https://app.fit-track.net/api

# JWT Secret - Replace with a secure random string (min 32 characters)
REACT_APP_JWT_SECRET=replace_with_secure_production_key_min_32chars

# Default language
REACT_APP_DEFAULT_LANGUAGE=en

# Environment marker
NODE_ENV=production

# Optional: Analytics ID if using analytics service
# ANALYTICS_ID=

# Optional: Error logging service credentials
# ERROR_REPORTING_KEY= 